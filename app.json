{
  "expo": {
    "scheme": "grocerybai",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you upload"
        }
      ],
      "expo-secure-store"
    ],
    "name": "Grocery Bai - a grocery list",
    "slug": "app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/gorcerybai.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/gorcerybai.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.grocerybai.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/gorcerybai.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.grocery.app"
    },
    "web": {
      "favicon": "./assets/gorcerybai.png"
    },
    "extra": {
      "eas": {
        "projectId": "0e73747a-ec4a-457f-af6f-331a3a52f8e3"
      }
    }
  }
}
